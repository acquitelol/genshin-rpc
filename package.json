{
  "name": "gluta",
  "productName": "Gluta",
  "version": "1.6.4",
  "description": "A discord Rich Presence script built for Genshin Impact made to be as easy as possible to use.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-arm": "electron-builder",
    "dist-intel": "electron-builder --x64",
    "dist-linux": "electron-builder --linux"
  },
  "keywords": [],
  "author": {
    "name": "Acquite",
    "email": "acquitefx@gmail.com"
  },
  "license": "MIT",
  "build": {
    "asar": false,
    "appId": "gluta",
    "mac": {
      "icon": "./assets/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 175,
          "y": 210
        },
        {
          "x": 350,
          "y": 210,
          "type": "link",
          "path": "./Applications"
        }
      ]
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "all"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "axios": "^0.25.0",
    "colorthief": "^2.3.2",
    "discord": "^0.8.2",
    "discord-rpc": "^4.0.1",
    "discord.js": "^13.6.0",
    "dominant-color": "^0.0.1",
    "electron-packager": "^15.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.2",
    "jshint": "^2.13.4",
    "node-fetch": "^2.6.7",
    "node-vibrant": "^3.2.1-alpha.1",
    "vibrant": "^0.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron/remote": "^2.0.1",
    "axios": "^0.25.0",
    "colorthief": "^2.3.2",
    "discord": "^0.8.2",
    "discord-rpc": "^4.0.1",
    "discord.js": "^13.6.0",
    "dominant-color": "^0.0.1",
    "electron": "^16.0.8",
    "electron-builder": "^22.14.5",
    "electron-packager": "^15.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.2",
    "node-vibrant": "^3.2.1-alpha.1",
    "vibrant": "^0.0.1"
  }
}
