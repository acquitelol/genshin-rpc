{
  "name": "gluta",
  "productName": "Gluta",
  "version": "1.6.55",
  "description": "A discord Rich Presence script built for Genshin Impact made to be as easy as possible to use.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist-arm": "electron-builder",
    "dist-intel": "electron-builder --x64",
    "dist-linux": "electron-builder --linux"
  },
  "keywords": [],
  "author": {
    "name": "Acquite",
    "email": "acquitefx@gmail.com"
  },
  "license": "MIT",
  "build": {
    "asar": false,
    "appId": "gluta",
    "mac": {
      "icon": "./assets/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 175,
          "y": 210
        },
        {
          "x": 350,
          "y": 210,
          "type": "link",
          "path": "./Applications"
        }
      ]
    }
  },
  "config": {},
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "colorthief": "^2.3.2",
    "discord": "^0.8.2",
    "discord-banner": "^1.6.7",
    "discord-rpc": "^4.0.1",
    "electron-packager": "^15.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.2",
    "jshint": "^2.13.4",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron/remote": "^2.0.1",
    "colorthief": "^2.3.2",
    "discord": "^0.8.2",
    "discord-rpc": "^4.0.1",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-packager": "^15.4.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.2"
  }
}
